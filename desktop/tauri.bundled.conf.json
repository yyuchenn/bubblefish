{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Bubblefish",
  "version": "0.1.0",
  "identifier": "com.yyuchenn.bubblefish",
  "build": {
    "frontendDist": "../frontend/build",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/plugins/*"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}